<textarea
        <% field.config.required && 'required' %>
        <% field.config.disabled && 'disabled'%>
        name="<%= key %>"
        id="form-<%= key %>"
        rows="10"><%-JSON.stringify(value)%>
</textarea>

<script>

    new EditNavigation({
        element: '<%=key%>-container',
        field: '<%=key%>',
        data: $('#form-<%=key%>').val()
    });

</script>