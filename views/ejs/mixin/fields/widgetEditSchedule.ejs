<textarea
        <%= field.config.required && 'required' %>
        <%= field.config.disabled && 'disabled'%>
        name="<%= key %>"
        id="form-<%= key %>"
        rows="10" hidden><%-JSON.stringify(value)%>
</textarea>

<script>
    var options<%=key%> = <%- JSON.stringify(field.config.options) %>
   new EditSchedule({
       ...options<%=key%>,
      element: '<%=key%>-container',
      field: '<%=key%>',
      data: $('#form-<%=key%>').val()
   });

</script>
